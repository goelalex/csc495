import { push } from 'connected-react-router'

import { AlertActions } from 'Actions';
import { AssessmentConstants } from 'Constants';
import { AssessmentServices } from 'Services';
import { History } from 'Helpers';

function getPatients() {


    return dispatch => {
        dispatch(request({ username }));
        dispatch()
        AssessmentServices.getPatients(username)
            .then(
                patients => {
                    dispatch(success(patients))
                },
                error => {
                    dispatch(failure(error.toString))
                })
    };

    function request(patients) { return { type: AssessmentActions.GET_PATIENTS_REQUEST, patients } }
    function success(patients) { return { type: AssessmentActions.GET_PATIENTS_SUCCESS, patients } }
    function failure(error) { return { type: AssessmentActions.GET_PATIENTS_ERROR, error } }
}

export const AssessmentActions = {
    getPatients,
};